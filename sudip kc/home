from collections.abc import Sequence
x = 13
if x > 4:
    if x < 14:
        print("x is between 4 to 14")
x = 10
if x > 5:
    if x < 20:
        print("x is between 5 and 20")
x = 10
if x > 5:
    if x < 20:print("x is between 5 and 20")
    x = 4
    
print("Greater than 5") if x > 5 else print("5 or less")
