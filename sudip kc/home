num1 =int(input("enter 1st num"))
num2 =int(input("enter 2nd num")) 
op=input("enter operators(+,-,*,/)")
if op=="+":
    print(num1+num2)
elif op== "-":
    print(num1-num2)
elif op=="*":
    print(num1*num2)
elif op=="/":
    print(num1/num2)
else:
    print("invalid op")
    
mark =int(input("enter a mark"))
if 90< mark < 100:
    print("excellent")
elif 80< mark < 90:
    print("very good")
elif 70 < mark< 80:
    print("good")
elif 60< mark < 70:
    print("better")
elif 50< mark<60:
    print("could be better")    
elif 40< mark < 50:
    print("pass")
else:
    print("fail")    
    
     
 